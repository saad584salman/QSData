{
  "name": "qsdata",
  "version": "1.0.0",
  "description": "Sample PERN stack application",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "dev:full": "npm run dev:backend & npm run dev:frontend",
    "dev:backend": "nodemon index.js",
    "dev:frontend": "vite",
    "dev:docker": "docker-compose -f docker-compose.dev.yml up --build",
    "dev:docker:clean": "docker-compose -f docker-compose.dev.yml down -v && docker-compose -f docker-compose.dev.yml up --build",
    "dev:start": "node scripts/dev-start.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:api": "node tests/api-test.js",
    "test:quick": "curl -s http://localhost:3000/health",
    "test:runner": "node tests/test-runner.js",
    "test:docker": "node tests/test-runner.js all docker",
    "test:local": "node tests/test-runner.js all local",
    "test:api-only": "node tests/test-runner.js api api",
    "test:unit": "node tests/test-runner.js unit local",
    "test:integration": "node tests/test-runner.js integration local",
    "test:docker-compose": "docker-compose -f docker-compose.test.yml up --build --abort-on-container-exit",
    "test:docker-clean": "docker-compose -f docker-compose.test.yml down -v",
    "db:init": "node scripts/init_db.js",
    "db:migrate": "knex migrate:latest",
    "db:migrate:rollback": "knex migrate:rollback",
    "db:seed": "knex seed:run",
    "db:refresh": "npm run db:migrate:rollback && npm run db:migrate && npm run db:seed",
    "db:setup": "npm run db:migrate && npm run db:seed",
    "db:reset": "npm run db:migrate:rollback && npm run db:setup",
    "refresh-exports": "node scripts/refreshViews.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "client": "vite",
    "client:build": "vite build",
    "type-check": "tsc --noEmit",
    "build": "npm run client:build",
    "docker:build": "docker-compose -f docker-compose.dev.yml build",
    "docker:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:logs": "docker-compose -f docker-compose.dev.yml logs -f",
    "docker:clean": "docker-compose -f docker-compose.dev.yml down -v && docker system prune -f"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "objection": "^3.0.1",
    "pg": "^8.11.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "joi": "^17.12.2",
    "moment": "^2.30.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "vite": "^5.2.0",
    "@vitejs/plugin-react": "^4.0.3",
    "@types/node": "^20.11.24",
    "@types/express": "^4.17.21",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/pg": "^8.11.0",
    "@types/uuid": "^10.0.0",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "typescript": "^5.3.3",
    "prettier": "^3.2.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "tailwindcss": "^3.4.1",
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.35",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10"
  }
}
